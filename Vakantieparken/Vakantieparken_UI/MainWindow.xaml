<Window x:Class="Vakantieparken_UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vakantieparken_UI"
        mc:Ignorable="d"
                Title="Startpagina" Height="250" Width="400">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" >
            <Button Content="Nieuwe Reservatie Maken" Width="200" Height="40" Click="NieuweReservatieMaken_Click"/>
            <Button Content="Reservaties Zoeken" Width="200" Height="40" Click="ReservatiesZoeken_Click"/>
            <Button Content="Huis in Onderhoud Plaatsen" Width="200" Height="40" Click="HuisInOnderhoudPlaatsen_Click"/>
            <Button Content="Geaffecteerde Klanten Tonen" Width="200" Height="40" Click="GeaffecteerdeKlantenTonen_Click"/>
        </StackPanel>
    </Grid>
</Window>
